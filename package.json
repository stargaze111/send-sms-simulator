{
    "name": "sms-micro-insurance",
    "version": "1.0.0",
    "author": "Ivan Cai",
    "description": "Package for the SMS Micro-insurance application",
    "scripts": {
        "start": "tsc & http-server",
        "preinstall": "npm install -g http-server",
        "lint": "tslint ./app/**/*.ts -t verbose",
        "test": "ng test",
        "docker-build": "docker build -t ng2-quickstart .",
              "tsc": "tsc",
	      "tsc:w": "tsc -w",
      "typings": "typings",
        "docker": "npm run docker-build && docker run -it --rm -p 3000:3000 -p 3001:3001 ng2-quickstart",
        "pree2e": "npm run webdriver:update",
        "e2e": "tsc && concurrently \"http-server -s\" \"protractor protractor.config.js\" --kill-others --success first",
        "postinstall": "typings install dt~underscore --global --save && typings install dt~es6-promise dt~es6-collections --global --save ",
        "webdriver:update": "webdriver-manager update"

    },
    "license": "ISC",
